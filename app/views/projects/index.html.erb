<h3>Ongoing projects</h3>
<%= link_to 'New Project', new_project_path %>
<br>
<br>

<table>
  <thead>
    <tr>
  
      <th>Title</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        
      </tr>

    
      <td><%= link_to 'Edit', edit_profile_path(project) %></td>
   

    <% end %>

  </tbody>
</table>


